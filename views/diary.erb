<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE en-note SYSTEM "http://xml.evernote.com/pub/enml2.dtd">
<en-note>
  <div><%= @date %></div>
  <%# binding.remote_pry %>
  <% @activities.each do |activity|%>
    <% if activity[:type] == 'tweet' %>
      <div>
        <div><%= activity[:created_at] %></div>
        <%= activity[:content] %>
      </div>
    <% end %>
  <% end%>
</en-note>